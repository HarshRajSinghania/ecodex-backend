{
  "name": "ecodex",
  "version": "1.0.0",
  "description": "A gamified learning platform for identifying plants and animals",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "express",
    "ecodex",
    "plants",
    "animals",
    "identification"
  ],
  "author": "Harsh Raj Singhania",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.10.2",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
